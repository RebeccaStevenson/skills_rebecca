# Marker Convert Settings
# Loaded automatically by scripts/marker_convert.sh unless --no-settings is used.
# CLI flags always override values here.

# Conversion defaults
MODE=auto
OUTPUT_FORMAT=markdown
PAGE_RANGE=
CONFIG_JSON=

# Processing toggles
DISABLE_IMAGE_EXTRACTION=false
DISABLE_MULTIPROCESSING=false

# Wrapper timeout (seconds): 0 disables wrapper timeout
MARKER_TIMEOUT=0

# LLM controls
USE_LLM=false
LLM_SERVICE=marker.services.claude.ClaudeService
MODEL_NAME=claude-sonnet-4-5
SERVICE_TIMEOUT=
CLAUDE_MODEL_NAME=claude-sonnet-4-5
GEMINI_MODEL_NAME=
# CLAUDE_API_KEY=
# OPENAI_API_KEY=
# GOOGLE_API_KEY=

# Chunk-mode prerequisite (only needed when mode=chunk)
# NUM_DEVICES=1

# Optional command overrides
# MARKER_BIN=marker
# MARKER_SINGLE_BIN=marker_single
# MARKER_CHUNK_BIN=marker_chunk_convert
